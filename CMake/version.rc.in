#define VS_VERSION_INFO 0x1L
#define VOS__WINDOWS32 0x4L
#define VFT_DLL 0x2L
#define VFT2_UNKNOWN 0x0L

#define VS_FF_DEBUG         0x00000001L
#define VS_FF_INFOINFERRED  0x00000010L
#define VS_FF_PATCHED       0x00000004L
#define VS_FF_PRERELEASE    0x00000002L
#define VS_FF_PRIVATEBUILD  0x00000008L
#define VS_FF_SPECIALBUILD  0x00000020L

#if defined(DEBUG)
    #define FILE_FLAGS 0x9L
#else
    #define FILE_FLAGS 0x0L
#endif

VS_VERSION_INFO VERSIONINFO
  FILEVERSION    @PROMETHEUS_FILEVERSION@
  PRODUCTVERSION @PROMETHEUS_VERSION@
  FILEFLAGSMASK  0x3fL
  FILEFLAGS      FILE_FLAGS
  FILEOS         VOS__WINDOWS32
  FILETYPE       VFT_DLL
BEGIN
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0809, 1200
    END
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "Research and Reverse-Engineering project of Overwatch! Compiled with @CMAKE_CXX_COMPILER_ID@ @CMAKE_CXX_COMPILER_VERSION@\0"
            VALUE "ProductVersion", "@PROMETHEUS_VERSION@\0"
            VALUE "FileVersion", "@PROMETHEUS_FILEVERSION@\0"
            VALUE "InternalName", "Prometheus\0"
            VALUE "ProductName", "Prometheus\0"
            VALUE "LegalCopyright", "Copyright (c) 2025 BreakingBread0.\0"
            VALUE "Licence", "MIT License\0"
            VALUE "Info", "https://github.com/BreakingBread0/prometheus/\0"
        END
    END
END